<div class="row my-5">
  <div class="col-7">
    <%= form_with url:admin_genres_path, model: genre, local: false do |f| %>
      <strong><%= f.label :name, "ジャンル名", class:'mr-5' %></strong>
      <%= f.text_field :name, placeholder: "ジャンル名", id:"text_field", class:'mr-5' %>
      <%= f.submit '新規登録', class:'btn btn-success' %>
    <% end %>
  </div>
</div>