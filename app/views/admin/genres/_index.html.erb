

<div class="row">
  <table class="table">
    <thead>
      <tr>
        <th>ジャンル</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% genres.each do |genre| %>
        <% unless genre.nil? %>
          <tr>
            <td><%= genre.name %></td>
            <td><%= link_to '編集する', edit_admin_genre_path(genre.id), class:'btn btn-success' %></td>
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>